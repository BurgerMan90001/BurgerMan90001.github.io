{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": [

      "node",
    ],
  },
  "include": ["src/**/*", "app.ts", "server.ts"],
  "exclude": ["node_modules"]
}